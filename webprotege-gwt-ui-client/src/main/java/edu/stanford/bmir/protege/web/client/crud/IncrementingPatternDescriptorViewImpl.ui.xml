<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>
    <ui:style>
        .main {
            display: flex;
            flex-direction: row;
            flex-wrap: nowrap;
        }
    </ui:style>
    <g:HTMLPanel addStyleNames="{wp.style.formGroupRow}">
        <g:HTMLPanel addStyleNames="{wp.style.formGroup} {wp.style.formGroupNarrow}">
            <g:Label text="Starting value" addStyleNames="{wp.style.formLabel}"/>
            <g:TextBox ui:field="startingValueField" value="0"/>
        </g:HTMLPanel>
        <g:HTMLPanel addStyleNames="{wp.style.formGroup} {wp.style.formGroupNarrow}">
            <g:Label text="Format" addStyleNames="{wp.style.formLabel}"/>
            <g:TextBox ui:field="formatField"/>
        </g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>